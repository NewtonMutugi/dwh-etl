		public void Main()
		{
            // TODO: Add your code here
            var fromAddress = new MailAddress("palladiumssisemail@gmail.com", "SSIS refresh alert Notification");
            var toAddress = new MailAddress("dennis.ndwiga@thepalladiumgroup.com", "");
            var toAddress1 = new MailAddress("palladiumssisemail@gmail.com", "");
            var toAddress2 = new MailAddress("dennismugo523@gmail.com", "");

            var toAddress3 = new MailAddress("paul.nthusi@thepalladiumgroup.com", "");
            var toAddress4 = new MailAddress("koske.kimuti@thepalladiumgroup.com", "");
            var toAddress5 = new MailAddress("andrine.njagi@thepalladiumgroup.com", "");
            var toAddress6 = new MailAddress("charles.bett@thepalladiumgroup.com", "");
            var toAddress7 = new MailAddress("mary.gikura@thepalladiumgroup.com", "");
            var toAddress8 = new MailAddress("Evans.Munene@thepalladiumgroup.com", "");
            var toAddress9 = new MailAddress("fridah.oyucho@thepalladiumgroup.com", "");





            const string fromPassword = "vokfrfzyzopolcur";





            var smtp = new SmtpClient
            {
                Host = "smtp.gmail.com",
                Port = 587,
                EnableSsl = true,
                DeliveryMethod = SmtpDeliveryMethod.Network,
                UseDefaultCredentials = false,
                Credentials = new NetworkCredential(fromAddress.Address, fromPassword),
                Timeout = 20000
            };





            MailMessage mailmsg = new MailMessage();
            mailmsg.IsBodyHtml = true;
            mailmsg.Subject = "SSIS Package has Started";
            mailmsg.Body = "This is to notify you that SSIS Refresh has started at " + DateTime.Now.ToString("h:mm:ss tt");
            mailmsg.From = new MailAddress(fromAddress.ToString());
            mailmsg.To.Add(toAddress);
            mailmsg.To.Add(toAddress1);
            mailmsg.To.Add(toAddress2);

            mailmsg.To.Add(toAddress3);
            mailmsg.To.Add(toAddress4);
            mailmsg.To.Add(toAddress5);
            mailmsg.To.Add(toAddress6);
            mailmsg.To.Add(toAddress7);
            mailmsg.To.Add(toAddress8);
            mailmsg.To.Add(toAddress9);





            {
                smtp.Send(mailmsg);
            }





            Dts.TaskResult = (int)ScriptResults.Success;
        }


        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
